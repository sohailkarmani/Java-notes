// Define a class box with length,wreadth and heigth as member variable.Also define setDimension() and showDimension() as member functions.

class Box
{
private int length,wreadth,heigth;
public void setDimension(int l,int w,int h)
{ length l; breadth b; heigth h }
public void showDimension()
{
System.out.println("L="+length);
System.out.println("B="+breagth);
System.out.println("H="+heigth);
}
}
class Example
{
public static void main()
{
Box smallBox=new Box();
smallBox.setDimension(10,12,5);
smallBox.showDimension();
smallBox=new Box();
smallBox.showDimension();
}
}
